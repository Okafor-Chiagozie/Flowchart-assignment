name = input("Your name: ")

if name == "Alice":
    print("Hi, Alice.")
elif name != "Alice":
    age = int(input("Your age: "))
    if age < 12:
        print("You are not Alice, kiddo.")
    elif age > 2000:
        print("Unlike you, Alice is not an undead, immortal vampire.")
    elif age > 100:
        print("You are not Alice, grannie")
    

